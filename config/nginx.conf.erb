worker_processes 1;

events {
    worker_connections 1024;
}

http {
    include       mime.types;
    default_type application/octet-stream;

    server {
        listen 5000;
        server_name localhost;

        location / {
            root /ĐỒ ÁN THIẾT KẾ WEB;  # Đảm bảo đường dẫn này đúng với thư mục chứa tệp tĩnh của bạn
            index index.html index.htm;
            try_files $uri $uri/ =404;
        }
    }
}
